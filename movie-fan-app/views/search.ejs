<%- include('head') %>
</head>
<body>
  <%- include('navbar') %>

	<div class="container">
		<div class="row">
			<div class="col-sm-12">
				<h1>Search Results for: <%= searchQuery %> </h1>
			</div>
			<div class="col-sm-12">
				<% results.forEach(result => { %>
					<a href="/movie/<%= result.id %>">
						<div class="col-md-3 col-sm-4 poster">
						<img src="<%= imageBaseUrl + result.poster_path %>" crossorigin/>
					</div>
					</a>
				<% }) %>
				
			</div>
		</div>
	</div>
</body>
</html>